<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Certificate border -->
  <rect x="50" y="50" width="300" height="200" fill="#E8F5E9" stroke="#4CAF50" stroke-width="8" rx="10"/>
  
  <!-- Quality seal -->
  <circle cx="200" cy="150" r="60" fill="#4CAF50" stroke="#388E3C" stroke-width="4"/>
  
  <!-- Checkmark symbol -->
  <path d="M170 150 L190 170 L230 130" stroke="white" stroke-width="8" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
  
  <!-- Text -->
  <text x="200" y="100" font-family="Arial" font-weight="bold" font-size="24" text-anchor="middle" fill="#333">QUALITY</text>
  <text x="200" y="130" font-family="Arial" font-weight="bold" font-size="24" text-anchor="middle" fill="#333">ASSURANCE</text>
  
  <!-- Date -->
  <text x="200" y="200" font-family="Arial" font-size="16" text-anchor="middle" fill="#666">Issued: 2026</text>
  
  <!-- Signature line -->
  <line x1="120" y1="220" x2="280" y2="220" stroke="#999" stroke-width="2"/>
  <text x="200" y="240" font-family="Arial" font-size="14" text-anchor="middle" fill="#666">Certifying Authority</text>
</svg>
<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Laboratory building -->
  <rect x="75" y="120" width="250" height="140" fill="#E3F2FD" stroke="#1976D2" stroke-width="6"/>
  
  <!-- Roof -->
  <polygon points="75,120 200,80 325,120" fill="#BBDEFB" stroke="#1976D2" stroke-width="6"/>
  
  <!-- Door -->
  <rect x="175" y="180" width="50" height="80" fill="#FFFFFF" stroke="#1976D2" stroke-width="4"/>
  <circle cx="200" cy="220" r="3" fill="#1976D2"/>
  
  <!-- Windows -->
  <rect x="100" y="140" width="60" height="60" fill="#E1F5FE" stroke="#0277BD" stroke-width="4"/>
  <rect x="240" y="140" width="60" height="60" fill="#E1F5FE" stroke="#0277BD" stroke-width="4"/>
  
  <!-- Lab equipment - Microscope -->
  <g transform="translate(130, 160)">
    <rect x="0" y="0" width="30" height="40" fill="#37474F"/>
    <rect x="5" y="-10" width="20" height="15" fill="#455A64"/>
    <circle cx="15" cy="45" r="8" fill="#78909C"/>
  </g>
  
  <!-- Lab equipment - Beakers -->
  <g transform="translate(270, 160)">
    <rect x="0" y="0" width="8" height="30" fill="none" stroke="#0277BD" stroke-width="2"/>
    <rect x="12" y="0" width="8" height="35" fill="none" stroke="#0277BD" stroke-width="2"/>
    <rect x="24" y="0" width="8" height="25" fill="none" stroke="#0277BD" stroke-width="2"/>
    
    <!-- Liquid in beakers -->
    <rect x="0" y="20" width="8" height="10" fill="#03A9F4"/>
    <rect x="12" y="25" width="8" height="10" fill="#03A9F4"/>
    <rect x="24" y="15" width="8" height="10" fill="#03A9F4"/>
  </g>
  
  <!-- Sign above door -->
  <rect x="165" y="160" width="70" height="20" fill="#FF5722" stroke="#D32F2F" stroke-width="2"/>
  <text x="200" y="174" font-family="Arial" font-size="12" text-anchor="middle" fill="#FFFFFF">LABORATORY</text>
</svg>
<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Greenhouse frame -->
  <rect x="50" y="100" width="300" height="150" fill="none" stroke="#8D6E63" stroke-width="8"/>
  
  <!-- Roof -->
  <polygon points="50,100 200,50 350,100" fill="#A1887F" stroke="#8D6E63" stroke-width="8"/>
  
  <!-- Door -->
  <rect x="180" y="170" width="40" height="80" fill="#CFD8DC" stroke="#8D6E63" stroke-width="4"/>
  <line x1="200" y1="170" x2="200" y2="250" stroke="#8D6E63" stroke-width="2"/>
  
  <!-- Windows -->
  <rect x="80" y="120" width="60" height="80" fill="#ECEFF1" stroke="#8D6E63" stroke-width="4"/>
  <rect x="260" y="120" width="60" height="80" fill="#ECEFF1" stroke="#8D6E63" stroke-width="4"/>
  
  <!-- Plants inside -->
  <ellipse cx="120" cy="200" rx="20" ry="30" fill="#66BB6A"/>
  <ellipse cx="160" cy="190" rx="15" ry="25" fill="#81C784"/>
  <ellipse cx="240" cy="210" rx="18" ry="28" fill="#66BB6A"/>
  <ellipse cx="280" cy="195" rx="22" ry="32" fill="#81C784"/>
  
  <!-- Soil -->
  <rect x="70" y="230" width="260" height="20" fill="#8D6E63"/>
</svg>
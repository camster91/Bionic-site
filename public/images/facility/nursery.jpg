<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Nursery building -->
  <rect x="75" y="120" width="250" height="140" fill="#E8F5E9" stroke="#2E7D32" stroke-width="6"/>
  
  <!-- Roof -->
  <polygon points="75,120 200,80 325,120" fill="#81C784" stroke="#2E7D32" stroke-width="6"/>
  
  <!-- Door -->
  <rect x="175" y="180" width="50" height="80" fill="#FFFFFF" stroke="#2E7D32" stroke-width="4"/>
  <circle cx="200" cy="220" r="3" fill="#2E7D32"/>
  
  <!-- Windows -->
  <rect x="100" y="140" width="60" height="60" fill="#E3F2FD" stroke="#1976D2" stroke-width="4"/>
  <rect x="240" y="140" width="60" height="60" fill="#E3F2FD" stroke="#1976D2" stroke-width="4"/>
  
  <!-- Plants in nursery -->
  <g id="plant1">
    <rect x="110" y="200" width="10" height="40" fill="#8D6E63"/>
    <ellipse cx="115" cy="195" rx="15" ry="20" fill="#66BB6A"/>
  </g>
  
  <g id="plant2">
    <rect x="130" y="210" width="8" height="35" fill="#8D6E63"/>
    <ellipse cx="134" cy="205" rx="12" ry="15" fill="#4CAF50"/>
  </g>
  
  <g id="plant3">
    <rect x="270" y="200" width="10" height="40" fill="#8D6E63"/>
    <ellipse cx="275" cy="195" rx="15" ry="20" fill="#66BB6A"/>
  </g>
  
  <g id="plant4">
    <rect x="290" y="210" width="8" height="35" fill="#8D6E63"/>
    <ellipse cx="294" cy="205" rx="12" ry="15" fill="#4CAF50"/>
  </g>
  
  <!-- Sign above door -->
  <rect x="165" y="160" width="70" height="20" fill="#FFEB3B" stroke="#F57F17" stroke-width="2"/>
  <text x="200" y="174" font-family="Arial" font-size="12" text-anchor="middle" fill="#F57F17">NURSERY</text>
</svg>